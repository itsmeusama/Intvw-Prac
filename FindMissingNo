class Codechef
{
    public static void findMissingNo(int arr[])
    {
        for(int i=1; i<arr.length;i++)
        {
            if(arr[i-1]!=i)
            {
                System.out.println("Here missing value is "+i);
                break;
            }
        }
        
    }
    
	public static void main (String[] args) throws java.lang.Exception
	{
		int arr[] = {1,2,3,4,5,6,8,9};
		findMissingNo(arr);
	}
}
