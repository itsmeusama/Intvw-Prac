//ArrayListExample
	
	List<String> list = new ArrayList<>();
		
		System.out.println(" Initially  Size of the List is: "+list.size());
		
		list.add("A");
		list.add("E");
		list.add("I");
		list.add("O");
		list.add("U");
		
		System.out.println(" After adding values Size of the List is: "+list.size());
		
		
		list.remove(4);

		System.out.println(" After Removing Size of the List is: "+list.size());
		
		
		for (String string : list) {
			System.out.println(string);
		}
