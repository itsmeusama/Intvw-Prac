public class Ex {

	public static void main(String[] args) {
		
		String arr[]= {"Apple","orange","Mango","orange"};
		String temp[]= {};
		
		for(int i=0; i<arr.length-1;i++)
		{
			for(int j=1; j<4; j++)
			{
				if(arr[i] != arr[j])    //AppleVsOrange
				{
					temp[j-1]=arr[j-1];  
				}
				else
					continue;
			}
				
		}
		for(String str: temp)
		{
			System.out.println(str);
		}

	}

}
