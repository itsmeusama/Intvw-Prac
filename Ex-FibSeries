 static void fib(int limit)
    {
    
        int result=0;
        int fib1=0;
        int fib2=1;
        for(int i=0;i<= limit;i++)
        {
               System.out.println(fib1);
               result=fib1+fib2;
               fib1=fib2;
               fib2=result;
        }
        //0,1,3,5
        //0,1,1,2,3,5,8,13
        //System.out.println("c");
    }
